{
  "extends": ["tslint:recommended"],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [true, ".prettierrc"],
    "no-var-requires": false,
    "no-console": [false, "log", "error"],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "trailing-comma": false,
    "arrow-parens": false
  }
}
